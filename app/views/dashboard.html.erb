<!DOCTYPE html>
<html>
  <head>
    <title>Boilerplate</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag 'application' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js' %>
    <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js' %>
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
  </head>
  <body class='admin'>
    <div id='app'>
      <router-view></router-view>
    </div>
    <%= javascript_pack_tag 'dashboard/application' %>
  </body>
</html>
